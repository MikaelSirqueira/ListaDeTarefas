{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "semi": true,
        "indent": [
          "error",
          4
        ],
        "linebreak-style": 0
      }
    ],
    "camelcase": "off",
    "no-useless-constructor": "off"
  }
}